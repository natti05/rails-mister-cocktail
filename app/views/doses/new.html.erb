<div class="container">
  <div class="card_links d-flex justify-content-end"> <%= link_to cocktails_path do %>
    <i class="fas fa-arrow-left fa-2x"></i>
    <% end %>
  </div>

    <h1>Your new dose</h1>
    <%= simple_form_for [@cocktail, @dose] do |f| %>
    <%= f.association :ingredient %>
    <%= f.input :description %>
    <%= f.button :submit, class: "button"%>
<% end %>
</div>




